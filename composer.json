{
    "name": "acme/laravel-app",
    "type": "project",
    "description": "Laravel + Filament + Vue",
    "require": {
        "php": "^8.3",
        "laravel/framework": "^11.0",
        "filament/filament": "^4.0",
        "inertiajs/inertia-laravel": "^2.0"
    },
    "require-dev": {
        "laravel/pint": "^1.10",
        "larastan/larastan": "^3.0",
        "pestphp/pest": "^2.6",
        "pestphp/pest-plugin-laravel": "^2.0",
        "peckphp/peck": "^0.1.3",
        "rector/rector": "^2.1",
        "mockery/mockery": "^1.6",
        "fakerphp/faker": "^1.24",
        "laravel/pail": "^1.2",
        "laravel/sail": "^1.45",
        "laravel/tinker": "^2.10",
        "laravel/wayfinder": "^0.1.11"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\": "database/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "format": "pint",
        "stan": "phpstan analyse",
        "test": "pest",
        "peck": "peck",
        "rector:dry": "rector --dry-run",
        "rector:apply": "rector",
        "post-autoload-dump": [
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
